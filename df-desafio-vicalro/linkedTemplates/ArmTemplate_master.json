{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-desafio-vicalro"
		},
		"ls_container_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_container'"
		},
		"ls_container_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://desafioazure.dfs.core.windows.net/"
		},
		"ls_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-desafio-vicalro.vault.azure.net/"
		},
		"ls_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0804-140340-fikkby3c"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-desafio-vicalro_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_container_accountKey": {
						"value": "[parameters('ls_container_accountKey')]"
					},
					"ls_container_properties_typeProperties_url": {
						"value": "[parameters('ls_container_properties_typeProperties_url')]"
					},
					"ls_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_properties_typeProperties_baseUrl')]"
					},
					"ls_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}